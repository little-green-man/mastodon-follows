{
    "name": "little-green-man/mastodon-follows",
    "description": "Mastodon Follows - find more information about those others follow so you can follow along too.",
    "keywords": [
        "mastodon",
        "follows",
        "php"
    ],
    "homepage": "https://github.com/little-green-man/mastodon-follows",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/little-green-man/mastodon-follows/issues",
        "source": "https://github.com/little-green-man/mastodon-follows"
    },
    "authors": [
        {
            "name": "Elliot Ali",
            "email": "elliot@lgm.ltd"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/http": "^10.0",
        "illuminate/view": "^10.0",
        "nunomaduro/termwind": "^1.15"
    },
    "require-dev": {
        "laravel/pint": "^1.5",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^1.22.3",
        "laravel-zero/framework": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "builds/mastodon-follows"
    ]
}
